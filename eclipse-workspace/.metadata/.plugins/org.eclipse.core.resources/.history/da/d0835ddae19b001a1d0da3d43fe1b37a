package ObjectConcepts;

public class Car {

	
	int mod;
	int wheel;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car a =new Car();// new Car() is the object for new class..a, b,c are references
		a.mod =10;
		a.wheel =20;
		System.out.println(a);
		Car b =new Car();
		
		

	}

}
